    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@swc-node/register": "^1.9.0",
    "@swc/cli": "0.3.12",
    "@swc/core": "1.5.0",
    "@types/express": "4.17.21",
    "@types/node": "20.12.7",
    "@types/sqlite3": "3.1.11",
    "eslint": "^8.57.0",
    "nodemon": "3.1.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1"
  },
  "engines": {
